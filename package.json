{
  "name": "next-redux-starter",
  "description": "Production ready boilerplate for Next.js",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "node server.js",
    "pretty": "prettier --write --tab-width 2 \"**/*.js\""
  },
  "lint-staged": {
    "*.js": [
      "yarn pretty"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@sentry/browser": "5.22.3",
    "@sentry/node": "^5.22.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "isomorphic-unfetch": "^3.0.0",
    "next": "12.2.0",
    "next-runtime-dotenv": "^1.3.0",
    "object-path": ">=0.11.5",
    "preact": "^10.5.15",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "react-ssr-prepass": "npm:preact-ssr-prepass@^1.2.0",
    "redux": "4.0.5",
    "redux-thunk": "^2.3.0",
    "sass": "^1.26.10"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "lint-staged": "10.2.13",
    "prettier": "2.4.1",
    "redux-logger": "^3.0.6"
  },
  "engines": {
    "node": ">=16"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
